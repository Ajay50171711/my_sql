CREATE TABLE employees (
    id INT,
    name VARCHAR(100),
    age INT,
    department VARCHAR(50)
);

INSERT INTO employees (id, name, age, department)
VALUES (1, 'Alice', 30, 'HR');

SELECT * FROM employees;

UPDATE employees
SET age = 31
WHERE id = 1;

DELETE FROM employees
WHERE id = 1;

SHOW TABLES;

